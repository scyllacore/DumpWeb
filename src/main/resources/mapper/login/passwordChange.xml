<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scyllacore.dumpWeb.commonModule.db.mapper.login.PasswordChangeMapper">

    <update id="updateUserPassword" parameterType="Login">
        UPDATE tsitepw
        SET userPW = SHA2(#{userPw}, 256)
        WHERE uuserID = #{uuserID}
    </update>

</mapper>



<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scyllacore.dumpweb.commonModule.db.mapper.DailyReportStep3SubMapper">
    <!--    &lt;!&ndash;tSheet table과 tSheet_sub table을 join하는 resultMap&ndash;&gt;
        <resultMap type="DailyReportMain"   id="DailyReportMainMapper">
            <id column="sheetID"             property="sheetID"/>
            <result column="CarNo"           property="CarNo"/>
            <result column="carSubmit"        property="carSubmit"/>
            <result column="carSubmitTel"    property="carSubmitTel"/>
            <result column="chk1"            property="chk1"/>
            <result column="date"            property="date"/>
            <result column="salesman"        property="salesman"/>
            &lt;!&ndash;
            collection  = 1 : 多
            association = 1 : 1
            &ndash;&gt;
        </resultMap>-->


    <!--운행 정보 저장-->
    <insert id="insertDailyReportSub" parameterType="DailyReportStep3Sub">
        <!--idx 자동->따로 작성 X-->
        insert into
        tSheet_sub
        (fromsite, item, Qty, Qtyup, Rem, sheetID2, tosite)
        values
        (#{fromsite}, #{item}, #{Qty}, #{Qtyup}, #{Rem}, #{sheetID2}, #{tosite})
    </insert>

    <!--    &lt;!&ndash;CarNo로 전체 조회하기, Join필요&ndash;&gt;
        <select id="findDailyReportList" resultType="DailyReportSub" parameterMap="DailyReportSubMapper">
            select * from tSheet where CarNo = #{CarNo}
        </select>-->

</mapper>